{
  "Logging": {
    "FileName": "light.log",
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "UpBoard": {
    "PwmPort": 1,
    "PwmPeriod": 10000
  },
  "IsUseInMemoryDatabase": true,
  "ConnectionString": "Data Source=light.db",
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://*:14567"
      },
      "HttpsDefaultCert": {
        "Url": "https://*:14568"
      }
    }
  },
  "UpTimer": {
    "TimeInterval": 10
  }
}
